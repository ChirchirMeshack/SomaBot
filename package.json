{
  "name": "soma-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon dist/index.js",
    "build": "tsc",
    "migrate": "node-pg-migrate up -m migrations",
    "rollback": "node-pg-migrate down 1 -m migrations",
    "test": "jest --coverage",
    "reset-db": "node-pg-migrate reset -m migrations",
    "seed": "ts-node scripts/seed.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.7",
    "typescript": "^5.0.0",
    "@types/pg": "^8.10.1",
    "node-pg-migrate": "^7.7.0",
    "@types/redis": "^4.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.11"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "nodemon": "^3.1.10",
    "winston": "^3.17.0",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.0.0",
    "jsonwebtoken": "^9.0.2"
  }
}
 